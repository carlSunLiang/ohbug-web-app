    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.14",
    "axios": "^0.19.2",
    "clsx": "^1.1.0",
    "js-cookie": "^2.2.1",
    "lint-staged": "^10.0.7",
    "moment": "^2.24.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-timeago": "^4.4.0",
    "redux-logger": "^3.0.6",
